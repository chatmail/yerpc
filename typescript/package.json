{
  "name": "yerpc",
  "type": "module",
  "version": "0.4.0",
  "author": "Franz Heinzmann <Frando>",
  "license": "Apache-2.0 or MIT",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/deltachat/yerpc"
  },
  "description": "An ergonomic JSON-RPC server library in Rust with autocreated TypeScript client",
  "files": [
    "**"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && esbuild dist/index.js --bundle --packages=external --format=cjs --outfile=dist/index.cjs",
    "lint": "prettier --check ./**.ts",
    "lint:fix": "prettier --write ./**.ts"
  },
  "dependencies": {
    "@types/ws": "^8.2.2",
    "isomorphic-ws": "^4.0.1",
    "typescript": "^4.6.3"
  },
  "optionalDependencies": {
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "esbuild": "^0.17.9",
    "prettier": "^2.6.2"
  }
}